<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:controller="sample.server.ServerController"
          xmlns:fx="http://javafx.com/fxml" alignment="top_left" hgap="10" vgap="10">
    <TextArea fx:id="clientMessages" GridPane.rowIndex="0" GridPane.columnIndex="0" prefHeight="200" prefWidth="400"
              editable="false">

    </TextArea>

    <Button fx:id="serverExit" text="Exit" GridPane.rowIndex="1" GridPane.columnIndex="0" onAction="#onExitClick" >

    </Button>
</GridPane>